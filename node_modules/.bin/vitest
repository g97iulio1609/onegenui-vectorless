#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/giulioleone/Sviluppo/genUi/json-render/node_modules/.pnpm/vitest@2.1.9_@types+node@22.19.6_happy-dom@20.4.0_jsdom@27.4.0_lightningcss@1.30.2/node_modules/vitest/node_modules:/Users/giulioleone/Sviluppo/genUi/json-render/node_modules/.pnpm/vitest@2.1.9_@types+node@22.19.6_happy-dom@20.4.0_jsdom@27.4.0_lightningcss@1.30.2/node_modules:/Users/giulioleone/Sviluppo/genUi/json-render/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/giulioleone/Sviluppo/genUi/json-render/node_modules/.pnpm/vitest@2.1.9_@types+node@22.19.6_happy-dom@20.4.0_jsdom@27.4.0_lightningcss@1.30.2/node_modules/vitest/node_modules:/Users/giulioleone/Sviluppo/genUi/json-render/node_modules/.pnpm/vitest@2.1.9_@types+node@22.19.6_happy-dom@20.4.0_jsdom@27.4.0_lightningcss@1.30.2/node_modules:/Users/giulioleone/Sviluppo/genUi/json-render/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
